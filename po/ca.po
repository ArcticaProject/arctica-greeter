msgstr "Accés"

#: ../src/user-list.vala:896
#, c-format
msgid "Login as %s"
msgstr "Accediu com a %s"

#: ../src/user-list.vala:900
msgid "Retry"
msgstr "Torna-ho a provar"

#: ../src/user-list.vala:901
#, c-format
msgid "Retry as %s"
msgstr "Torna-ho a provar com a %s"

#: ../src/user-list.vala:945
