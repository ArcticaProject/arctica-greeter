#, c-format
msgid "Login as %s"
msgstr "%s سۈپىتىدە كىرىڭ"

#: ../src/user-list.vala:900
msgid "Retry"
msgstr "قايتا سىنا"

#: ../src/user-list.vala:901
#, c-format
